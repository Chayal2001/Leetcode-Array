function sort_array(arr,length){
/*Function to sort the array in ascending order
After sorting return the sorted array */

    for(var i=1; i<length; i++){
    var temp = arr[i];
    var j = i-1
    while(j>=0 &&arr[j]>temp){
        arr[j+1]= arr[j];
        j--;
    }
    arr[j+1]=temp;
}
return arr;
}
